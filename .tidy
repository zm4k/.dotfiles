indent: auto
indent-spaces: 2
indent-attributes: no
indent-with-tabs: no
vertical-space: no
wrap-attributes: no
wrap: 79
doctype: auto
drop-empty-elements: no
drop-empty-paras: no
markup: yes
output-html: yes
output-xml: no
input-xml: no
show-warnings: yes
numeric-entities: yes
quote-marks: no
quote-nbsp: no
quote-ampersand: no
break-before-br: no
uppercase-tags: no
uppercase-attributes: no
char-encoding: utf8
new-inline-tags: cfif, cfelse, math, mroot, mrow, mi, mn, mo, msqrt, mfrac, msubsup, munderover, munder, mover, mmultiscripts, msup, msub, mtext, mprescripts, mtable, mtr, mtd, mth
new-blocklevel-tags: cfoutput, cfquery
new-empty-tags: cfelse 
tidy-mark: no
write-back: no
error-file: ak-tidy-error
bare: no
merge-divs: no
merge-spans: no
